<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.OracleDialect</property>
    <property name="hibernate.connection.driver_class">oracle.jdbc.OracleDriver</property>
    <property name="hibernate.connection.url">jdbc:oracle:thin:@DEVELOP-TEST:1521:XLP</property>
    <property name="hibernate.connection.username">importer</property>
    <property name="hibernate.connection.password">impwd</property>
    <property name="show_sql">true</property> 
    <mapping resource="com/se/mapping/TblSeUsers.hbm.xml"/>
    <mapping resource="com/se/mapping/TblSeFunctions.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
